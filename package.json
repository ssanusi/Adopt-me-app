{
  "name": "adopt-me",
  "version": "1.0.0",
  "main": "index.tsx",
  "license": "MIT",
  "scripts": {
    "clear-build-cache": "rm -rf .cache/ dist/",
    "dev": "parcel src/index.html",
    "dev:mock": "cross-env PET_MOCK=mock parcel src/index.html",
    "format": "tslint --project",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "typecheck": "tsc --noEmit"
